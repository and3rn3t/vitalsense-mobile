DEBUG_INFORMATION_FORMAT=dwarf

// Health Data Settings
HEALTH_DATA_SYNC_ENABLED=true
HEALTH_DATA_MOCK_MODE=true

// Analytics
ANALYTICS_ENABLED=false
ANALYTICS_DEBUG_MODE=true
